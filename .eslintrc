{
  "extends": ["airbnb", "prettier"],
  "rules": {
    "strict": [0],
    "func-names": [0],
    "no-param-reassign": [0],
    "global-require": [0],
    "no-console": [0],
    "import/no-dynamic-require": [0],
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
      }
    ]
  }
}
